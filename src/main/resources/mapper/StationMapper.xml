<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="train.mapper.StationMapper">


    <select id="confirmStationsByStationName" parameterType="String" resultType="java.lang.String">
        select station_name
        from station
        where station_name like '%${station_name}%';
    </select>

</mapper>